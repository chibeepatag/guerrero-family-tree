<p style="color: green"><%= notice %></p>

<h1>Members</h1>
<div id="members">
  <table>
    <tr><td>id</td><td>name</td><td>birth</td><td>death</td><td>in_law</td><td>nicknames</td><td>Parent</td><td>Parent ID</td><td>Spouse</td><td>Spouse ID</td></tr>
  
  <% @members.each do |member| %>
    <tr id="<%= dom_id member %>">
    <%= render member %>
    <td><%= link_to "Show this member", member %></td>
  </tr>
  <% end %>
  </table>
</div>

<%= link_to "New member", new_member_path %>